{
    "version": 2,
    "framework": null,
    "builds": [
      {
        "src": "api/index.php",
        "use": "vercel-php@0.7.2"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "buildCommand": "composer install --optimize-autoloader && php artisan route:cache && php artisan view:cache"
  }
